#include <Servo.h>
 
Servo servoLeft;
Servo servoRight;
 
void setup() {
 servoLeft.attach(13);
 servoRight.attach(12);
 servoLeft.writeMicroseconds(1700);
 servoRight.writeMicroseconds(1300);
 delay(2500);
 servoLeft.writeMicroseconds(1700);
 servoRight.writeMicroseconds(1700);
 delay(680);
 servoLeft.writeMicroseconds(1700);
 servoRight.writeMicroseconds(1300);
 delay(2500);
 servoLeft.writeMicroseconds(1700);
 servoRight.writeMicroseconds(1700);
 delay(680);
 servoLeft.writeMicroseconds(1700);
 servoRight.writeMicroseconds(1300);
 delay(2500);
 servoLeft.detach();
 servoRight.detach();
}
 
void loop() {
}
